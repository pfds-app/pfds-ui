{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 160,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["react", "^@core/(.*)$", "<THIRD_PARTY_MODULES>", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
